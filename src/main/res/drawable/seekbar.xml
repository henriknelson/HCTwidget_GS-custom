<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <stroke android:width="1px" android:color="#00ffffff"/>
            <corners android:radius="2px"/>
            <gradient android:startColor="#30ffffff" android:endColor="#30ffffff" android:angle="270" android:centerY="0.3" android:centerColor="#30ffffff"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <stroke android:width="1px" android:color="#31e27c"/>
                <corners android:radius="2px"/>
                <gradient android:startColor="#31e27c" android:endColor="#31e27c" android:angle="270" android:centerY="0.3" android:centerColor="#31e27c"/>
            </shape>
        </clip>
    </item>
</layer-list>
